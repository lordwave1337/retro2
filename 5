public class Main {
    public static void main(String[] args) {
        int[] arr = createArray(10, 5);
        
        for (int num : arr) {
            System.out.print(num + " ");
        }
    }
    
    public static int[] createArray(int len, int initialValue) {
        int[] arr = new int[len];
        
        for (int i = 0; i < len; i++) {
            arr[i] = initialValue;
        }
        
        return arr;
    }
}
